					"DatabaseName": {
						"type": "String",
						"defaultValue": "AdventureWorks2012"
					},
					"UserName": {
						"type": "String",
						"defaultValue": "DESKTOP-8SNQEQV\\gayat"
					},
					"Password": {
						"type": "String",
						"defaultValue": "Seselemento_98"
					},
					"user": {
						"type": "String",
						"defaultValue": "gayat"
					}
				},
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": {
						"type": "SecureString",
